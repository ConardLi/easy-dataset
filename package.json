{
  "name": "easy-dataset",
  "version": "1.1.0",
  "private": true,
  "author": {
    "name": "ConardLi",
    "url": "https://github.com/ConardLi"
  },
  "homepage": "https://github.com/ConardLi/easy-dataset",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "package": "node ./desktop/scripts/package.js",
    "electron": "electron .",
    "electron-dev": "concurrently \"pnpm dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron-build": "next build && electron-builder",
    "electron-pack": "electron-builder --dir",
    "electron-dist": "electron-builder"
  },
  "bin": "desktop/server.js",
  "pkg": {
    "assets": [
      ".next/**/*",
      "public/**/*",
      "locales/**/*",
      "package.json",
      "node_modules/next/**/*"
    ],
    "targets": [
      "node18-macos-arm64",
      "node18-macos-x64",
      "node18-win-x64",
      "node18-linux-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/inter": "^5.0.16",
    "@fontsource/jetbrains-mono": "^5.0.18",
    "@mui/icons-material": "5.16.14",
    "@mui/lab": "5.0.0-alpha.175",
    "@mui/material": "5.16.14",
    "@opendocsg/pdf2md": "^0.2.1",
    "framer-motion": "^12.4.10",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "mammoth": "^1.9.0",
    "next": "14.1.0",
    "next-themes": "^0.2.1",
    "opener": "^1.5.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^15.4.1",
    "react-markdown": "^10.0.1",
    "turndown": "^7.2.0",
    "uuid": "^11.1.0"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^29.4.6",
    "electron-builder": "^24.13.3",
    "pkg": "^5.8.1",
    "wait-on": "^7.2.0"
  },
  "main": "electron/main.js",
  "description": "一个用于创建大模型微调数据集的应用程序",
  "build": {
    "appId": "com.easydataset.app",
    "productName": "Easy Dataset",
    "files": [
      ".next/**/*",
      "!.next/cache/**/*",
      "public/**/*",
      "locales/**/*",
      "package.json",
      "electron/**/*",
      "node_modules/**/*",
      "!node_modules/.cache/**/*",
      "!node_modules/.bin/**/*",
      "!node_modules/.vite/**/*",
      "!**/*.{md,d.ts,map}",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}"
    ],
    "directories": {
      "buildResources": "public",
      "output": "dist"
    },
    "asar": true,
    "compression": "maximum",
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": [
        {
          "target": "dmg",
          "arch": ["arm64"]
        }
      ],
      "electronLanguages": ["zh_CN", "en"]
    }
  }
}
